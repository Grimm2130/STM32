	//Exercise
	int sum = 0;
	for(int i = 0; i < 3; i++){
		int temp = 0;
		// Wait for the input
		scanf("%d", &temp);
		// Add to the sum
		sum += temp;
	}
	printf("The mean is: %f", (float)(sum/3));
