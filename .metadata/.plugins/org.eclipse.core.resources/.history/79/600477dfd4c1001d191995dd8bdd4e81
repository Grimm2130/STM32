#include <stdint.h>
#include "stm32f4xx.h"
#include "stm32f4xx_gpio_driver.h"

int main(void)
{
	gpio_hndlr_t* gpio_A = (gpio_hndlr_t*) GPIO_D_BASE_ADDR;

//	// instantiate the gpio
	GPIOx_handler_init(gpio_A, 12, OUTPUT);
//
//	// Enable ahb1 clock for gpio
	GPIOx_clk_en(gpio_A);

	gpio_A->OD_R |= (1 << 8);

	/* Loop forever */
	for(;;);
}
